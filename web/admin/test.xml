	<queryString language="SQL">
		<![CDATA[SELECT
     accom.`accId` AS accom_accId,
     accom.`accName` AS accom_accName,
     place.`plId` AS place_plId,
     place.`plName` AS place_plName,
     ztest.`PId` AS ztest_PId,
     ztest.`Adults` AS ztest_Adults,
     ztest.`Children` AS ztest_Children,
     ztest.`fName` AS ztest_fName,
     ztest.`lName` AS ztest_lName,
     ztest.`Duration` AS ztest_Duration,
     zday.`pId` AS zday_pId,
     zday.`DayNo` AS zday_DayNo,
     zday.`plId` AS zday_plId,
     zselectedplaces.`Pid` AS zselectedplaces_Pid,
     zselectedplaces.`dayNo` AS zselectedplaces_dayNo,
     zselectedplaces.`place` AS zselectedplaces_place,
     zplace.`placeId` AS zplace_placeId,
     zplace.`Name` AS zplace_Name,
     zplace.`desc` AS zplace_desc,
     zdayacco.`pId` AS zdayacco_pId,
     zdayacco.`AccoId` AS zdayacco_AccoId,
     zdayacco.`double` AS zdayacco_double,
     zdayacco.`single` AS zdayacco_single,
     zdayacco.`nonVeg` AS zdayacco_nonVeg,
     zdayacco.`Veg` AS zdayacco_Veg,
     zdayacco.`dayNo` AS zdayacco_dayNo
FROM
     `zday` zday INNER JOIN `ztest` ztest ON zday.`pId` = ztest.`PId`
     INNER JOIN `place` place ON zday.`plId` = place.`plId`
     INNER JOIN `zselectedplaces` zselectedplaces ON zday.`pId` = zselectedplaces.`Pid`
     AND zselectedplaces.`dayNo` = zday.`DayNo`
     INNER JOIN `zdayacco` zdayacco ON zday.`DayNo` = zdayacco.`dayNo`
     AND zdayacco.`pId` = zday.`pId`
     INNER JOIN `accom` accom ON zdayacco.`AccoId` = accom.`accId`
     INNER JOIN `zplace` zplace ON zselectedplaces.`place` = zplace.`placeId`
WHERE
    ztest. PId  =$P{parameter1}]]>
	</queryString>
